@code
{  
    protected override async Task Process(Mat startFrameMat, Mat endFrameMat)
    { 
        using var grayMat = new Mat();
        Cv2.CvtColor(startFrameMat, grayMat, ColorConversionCodes.BGR2GRAY);

        await SetDstImage(grayMat); 
    } 
}
